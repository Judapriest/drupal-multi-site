{
  "extends": "../../../web/.eslintrc.json",
  // Disable ES6 for the moment.
  "env": {
    "es6": false
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  },
  "rules": {
    "strict": ["error", "global"],
    // Override ES6 rules.
    "no-dupe-class-members": [0],
    "no-new-symbol": [0],
    "no-use-before-define": [0],
    "no-var": [0],
    "object-shorthand": [0],
    "prefer-arrow-callback": [0],
    "prefer-numeric-literals": [0],
    "prefer-rest-params": [0],
    "prefer-spread": [0],
    "prefer-template": [0],
    "symbol-description": [0]
  }
}
